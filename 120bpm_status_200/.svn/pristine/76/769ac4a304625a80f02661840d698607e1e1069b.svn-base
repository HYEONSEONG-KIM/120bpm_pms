@charset "UTF-8";

#prtInfoArea{
	display: grid;
	grid-template-columns : 2fr 0.1fr 2fr;
	margin: 2%;
	text-align: center;
	width: 95%;
}
#memberInfo{
		display: grid;
		grid-template-columns : 0.1fr 4fr 0.1fr;
		margin: 2%;
		text-align: center;
		width: 95%;
		background: #ffffff;	
	
}
.content{
	background: #ffffff;
	border-radius: 7px;
}
#memberInfo td{
	text-align: center;	
}
.progress{
	
	margin-top: 1.2rem;
	
}