@charset "UTF-8";

html,body{
	width : 100%;
	height : 100%;
	background-color: #FCFCFC;

}

#parent{
	width: 100%;
   	height: 100%;
    display: grid;
    align-items: center; 
    justify-content: center; 
  font-family: 'Poor Story', cursive;
    text-align: center;
}

#messageArea{
	background-color: white;
	border-radius : 20px;
	box-shadow : 8px 8px 8px 8px lightgray;
	width: 1200px;
	height: 800px;
	
}

#message{
	margin-top: 15%;
	
}

#statusCode{
	font-size: 200px;
}
#statusMessage{
	font-size: 30px;	
}

